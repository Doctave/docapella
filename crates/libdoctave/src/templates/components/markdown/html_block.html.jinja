{% from "components/markdown.html.jinja" import markdown %}
{# prettier-ignore-start #}
<{{ node.kind.data.name }}
  {% for attr in node.kind.data.attributes %}
    {{ attr.key }}="{{ attr.value }}"
  {% endfor %}
  >
  {% for child in node.children %}
    {{ markdown(child) }}
  {% endfor %}
</{{ node.kind.data.name }}>
{# prettier-ignore-end #}
